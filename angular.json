{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "cli": {
    "analytics": "8b2be1e6-8acd-46f9-9e40-358a9292c9a8"
  },
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "rest-admin": {
      "projectType": "library",
      "root": "projects/rest-admin",
      "sourceRoot": "projects/rest-admin/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/rest-admin/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/rest-admin/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/rest-admin/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rest-admin/src/test.ts",
            "tsConfig": "projects/rest-admin/tsconfig.spec.json",
            "karmaConfig": "projects/rest-admin/karma.conf.js"
          }
        }
      }
    },
    "rest-admin-example": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        },
        "@schematics/angular:application": {
          "strict": true
        }
      },
      "root": "projects/rest-admin-example",
      "sourceRoot": "projects/rest-admin-example/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/rest-admin-example",
            "index": "projects/rest-admin-example/src/index.html",
            "main": "projects/rest-admin-example/src/main.ts",
            "polyfills": "projects/rest-admin-example/src/polyfills.ts",
            "tsConfig": "projects/rest-admin-example/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/rest-admin-example/src/assets/*",
              "projects/rest-admin-example/src/favicon.ico",
              "projects/rest-admin-example/src/assets",
              {
                "glob": "**/*",
                "input": "node_modules/leaflet/dist/images",
                "output": "/assets/i18n/*"
              }
            ],
            "styles": [
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/typeface-exo/index.css",
              "node_modules/roboto-fontface/css/roboto/roboto-fontface.css",
              "node_modules/ionicons/scss/ionicons.scss",
              "node_modules/socicon/css/socicon.css",
              "node_modules/nebular-icons/scss/nebular-icons.scss",
              "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "node_modules/leaflet/dist/leaflet.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "projects/rest-admin/src/lib/@theme/styles/styles.scss",
              "projects/rest-admin-example/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "projects/rest-admin-example/src/environments/environment.ts",
                  "with": "projects/rest-admin-example/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "rest-admin-example:build:production"
            },
            "development": {
              "browserTarget": "rest-admin-example:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "rest-admin-example:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/rest-admin-example/src/test.ts",
            "polyfills": "projects/rest-admin-example/src/polyfills.ts",
            "tsConfig": "projects/rest-admin-example/tsconfig.spec.json",
            "karmaConfig": "projects/rest-admin-example/karma.conf.js",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/rest-admin-example/src/favicon.ico",
              "projects/rest-admin-example/src/assets"
            ],
            "styles": [
              "projects/rest-admin-example/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "defaultProject": "rest-admin"
}
